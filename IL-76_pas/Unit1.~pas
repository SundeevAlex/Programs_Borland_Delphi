form1.Close;
end;

procedure TForm1.Edit1KeyPress(Sender: TObject; var Key: Char);
begin
case Key of
#8,'0'..'9' : ; // цифры и клавиша <Backspace>
#13: edit2.SetFocus;
else key := Chr(0); // остальные символы запрещены
end;
end;

procedure TForm1.ComboBox1KeyPress(Sender: TObject; var Key: Char);
begin
ComboBox1Select(Sender);
end;

procedure TForm1.Button2Click(Sender: TObject);
var
 ppx,ppy,ppi:longint; 
begin
if memo1.Text='' then exit;
button2.enabled:=false;
                               // П Е Ч А Т Ь графический режиим
 {if FontDialog1.Execute=true then
   begin
    Printer.Canvas.Font.Name:=fontdialog1.font.name;
    Printer.Canvas.Font.Size:=fontdialog1.font.size;
   end
   else begin button2.enabled:=true;exit;end;}

   begin
   Printer.Canvas.Font.Name:='Courier New';
   Printer.Canvas.Font.Size:=12;
   Printer.BeginDoc;
   ppx:=5; ppy:=5;
   for ppi:=1 to Memo1.Lines.Count{-2} do
     begin
     Printer.Canvas.TextOut(ppx,ppy,Memo1.Lines[ppi-1]);
     ppy:=ppy+Printer.Canvas.TextExtent(Memo1.Lines[ppi-1]).cy;  // -1 не должно быть
     if (ppi mod 55=0)and(ppi<>0)and(ppi<>Memo1.Lines.Count{-2}) then
        begin
        Printer.EndDoc;
       //MessageDlg('Вставьте новый лист.', mtinformation,[mbOk],0);
        Printer.BeginDoc;
        ppy:=5;
        end;
     end;
   Printer.EndDoc;
   end;
button2.enabled:=true;
memo1.SetFocus;
end;

